     (OUTSEM (LAM LF (LAM P (P LF)))) (INDEX STR) (PARAM 0.03))
    ((KEY 6) (INSYN ((BCAT NP) (FEATS NIL))) (INSEM LF)
     (OUTSYN
      ((((BCAT S) (FEATS NIL)) (DIR BS) (MODAL ALL) ((BCAT NP) (FEATS NIL)))
